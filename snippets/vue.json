      "\t<div>",
      "\t\t<h2>{{ count }}</h2>",
      "\t\t<div><button @click=\"count++\" class=\"btn\">click</button></div>",
      "\t</div>",
      "</template>\n",
      "<script setup>",
      "\timport { ref } from 'vue';",
      "\tconst count = ref(0);",
